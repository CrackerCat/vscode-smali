{
  "name": "smali",
  "displayName": "Smali",
  "description": "Smali language support for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "crazywolf",
  "icon": "images/smaliIcon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/CrazyWolf2014/vscode-qml/issues"
  },
  "homepage": "https://github.com/CrazyWolf2014/vscode-qml/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrazyWolf2014/vscode-smali.git"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "smali",
        "aliases": [
          "Smali"
        ],
        "extensions": [
          ".smali"
        ],
        "configuration": "./smali.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "smali",
        "scopeName": "source.smali",
        "path": "./syntaxes/smali.tmLanguage"
      }
    ]
  }
}